{
    "manifest_version": 2,
    "name": "Netatmo CO₂ Indicator",
    "short_name": "CO₂ Indicator",
    "version": "1.6.7",
    "description": "See your CO₂ level in the browser toolbar",
    "permissions": [
        "identity",
        "storage",
        "alarms",
        "theme"
    ],
    "optional_permissions": [
        "notifications"
    ],
    "icons": {
        "64": "icon.svg"
    },
    "browser_action": {
        "default_title": "Netatmo CO₂ Measurement",
        "default_icon": "status/gray.svg"
    },
    "background": {
        "scripts": [
            "config.js",
            "background.js"
        ]
    },
    "options_ui": {
        "page": "options.html",
        "browser_style": true
    },
    "applications": {
        "gecko": {
            "id": "{4b69f471-a76c-4355-be23-8d9b0c9802db}",
            "strict_min_version": "62.0"
        }
    }
}
